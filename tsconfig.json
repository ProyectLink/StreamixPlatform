{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {    
      "baseUrl": "./",
      "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@styles/*": ["src/styles/*"],
      "@hooks/*": ["src/hooks/*"],
      "@router/*": ["src/app/router/*"],
      "@assets/*": ["src/assets/*"],
      "@atoms/*": ["src/components/atoms/*"],
      "@layouts/*": ["src/app/layouts/*"],
      "@utils/*": ["src/utils/*"],
      "@store/*": ["src/store/*"],
      "@types/*": ["src/types/*"],

    },

  }
}
